{
    "env": {
        "node": true
    },
    "rules": {
        "semi": 2,
        "quotes": [2, "double"],
        "no-undef": 2,
        "no-caller": 2,
        "no-use-before-define": [2, "nofunc"],
        "no-multi-spaces": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "key-spacing": 0,
        "no-alert": 0,
        "no-shadow": 0,
        "new-cap": 0,
        "camelcase": 2,
        "strict": 0,
        "consistent-return": 0,
        "no-unused-vars": 1,
        "space-before-keywords": [2, "always"],
        "space-after-keywords": 2
    }
}
